    <script src="~/Content/Customer_Side/js/jquery-3.2.1.min.js"></script>
    <script src="~/Content/Customer_Side/js/popper.min.js"></script>
    <script src="~/Content/Customer_Side/js/bootstrap.min.js"></script>
    <!-- ALL PLUGINS -->
    <script src="~/Content/Customer_Side/js/jquery.superslides.min.js"></script>
    <script src="~/Content/Customer_Side/js/images-loded.min.js"></script>
    <script src="~/Content/Customer_Side/js/isotope.min.js"></script>
    <script src="~/Content/Customer_Side/js/baguetteBox.min.js"></script>
    <script src="~/Content/Customer_Side/js/form-validator.min.js"></script>
    <script src="~/Content/Customer_Side/js/contact-form-script.js"></script>
    <script src="~/Content/Customer_Side/js/custom.js"></script>
